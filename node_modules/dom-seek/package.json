{
  "name": "dom-seek",
  "version": "4.0.3",
  "description": "Text traversal for HTML documents.",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && BABEL_ENV=test karma start --single-run",
    "prepublish": "babel -d lib src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tilgovi/dom-seek.git"
  },
  "keywords": [
    "dom",
    "text",
    "seek"
  ],
  "author": "Randall Leeds <randall@bleeds.info>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tilgovi/dom-seek/issues"
  },
  "homepage": "https://github.com/tilgovi/dom-seek",
  "dependencies": {
    "ancestors": "0.0.3",
    "index-of": "^0.2.0"
  },
  "devDependencies": {
    "assertive-chai": "^2.0.2",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "codecov.io": "^0.1.6",
    "dom-node-iterator": "^3.5.0",
    "eslint": "^3.3.1",
    "karma": "^1.2.0",
    "karma-browserify": "^5.0.1",
    "karma-coverage": "^1.1.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.4",
    "watchify": "^3.7.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          "transform-es3-member-expression-literals",
          "transform-es3-property-literals"
        ]
      }
    },
    "plugins": [
      "transform-es3-member-expression-literals",
      "transform-es3-property-literals"
    ],
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ]
    ],
    "sourceMaps": "inline"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": 0
    }
  }
}
